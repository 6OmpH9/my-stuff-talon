app: /Obsidian/
-
# tab navigation
tag(): user.tabs

go back:
    key(ctrl-alt-b)

go forward:
    key(ctrl-alt-f)    
    
# UI navigation
sidebar:
    key(ctrl-b)

search open:
    key(ctrl-shift-f)
    
show settings:
    key(ctrl-,)    

go new window:
    key(ctrl-shift-;)
    
# text formatting    
make task:
    key(ctrl-l)    

make bold:
    key(ctrl-b)    

make italic:
    key(ctrl-i)

# delete current file    
recycle it:
    key(ctrl-backspace)